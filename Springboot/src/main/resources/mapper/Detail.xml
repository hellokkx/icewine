<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot.mapper.DetailMapper">



    <select id="list" resultType="com.example.springboot.entity.Detail">
        select * from detail
        where oid=#{id}
        order by did desc
    </select>

    <select id="listByCondition" resultType="com.example.springboot.entity.Detail">
        select * from detail
        order by did desc
    </select>

    <select id="getById" resultType="com.example.springboot.entity.Detail">
        select * from admin where id = #{id}
    </select>


</mapper>